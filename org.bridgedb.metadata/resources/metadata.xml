<?xml version="1.0" encoding="windows-1252"?>

<metadata>
    <resource name="DatasetDescription" type="http://rdfs.org/ns/void#Dataset">
        <propery
            name="Title"
            predicate="http://purl.org/dc/terms/title"
            class="String"
            requirementLevel = "MUST"
        />          
        <propery
            name="Description"
            predicate="http://purl.org/dc/terms/description"
            class="String"
            requirementLevel = "MUST"
        />          
        <propery
            name="Homepage"
            predicate="http://xmlns.com/foaf/0.1/homepage"
            class="Uri"
            requirementLevel = "MUST"
        />          
        <propery
            name="Page"
            predicate="http://xmlns.com/foaf/0.1/page"
            class="Uri"
            requirementLevel = "MAY"
        />          
        <propery
            name="License"
            predicate="http://purl.org/dc/terms/license"
            class="Uri"
            requirementLevel = "MUST"
        />          
        <propery
            name="Namespace"
            predicate="http://rdfs.org/ns/void#uriSpace"
            class="String"
            requirementLevel = "MUST"
        />          
        <propery
            name="Version"
            predicate="http://purl.org/pav/2.0/version"
            class="String"
            requirementLevel = "MAY"
        />          
        <alternatives name = "Provenance" requirementLevel = "MUST">
        <group name = "Original Location">
            <propery
                name="Publisher"
                predicate="http://purl.org/dc/terms/publisher"
                class="Uri"
                requirementLevel = "MAY"
            />          
            <propery
                name="Created"
                predicate="http://purl.org/dc/terms/created"
                class="Date"
                requirementLevel = "MAY"
            />          
            <propery
                name="Modified"
                predicate="http://purl.org/dc/terms/modified"
                class="Date"
                requirementLevel = "MAY"
            />
        </group>
        <group name = "Retrieved">
            <propery
                name="Retrieved From"
                predicate="http://purl.org/pav/2.0/retrievedFrom"
                class="Uri"
                requirementLevel = "MAY"
            />          
            <propery
                name="Retrieved On"
                predicate="http://purl.org/pav/2.0/retrievedOn"
                class="Date"
                requirementLevel = "MAY"
            />          
            <propery
                name="Retrieved By"
                predicate="http://purl.org/pav/2.0/retrievedBy"
                class="Uri"
                requirementLevel = "MAY"
            />     
        </group>
            <group name = "imported">
                <propery
                    name="Imported From"
                    predicate="http://purl.org/pav/2.0/importedFrom"
                    class="Uri"
                    requirementLevel = "MAY"
                />          
                <propery
                    name="Imported On"
                    predicate="http://purl.org/pav/2.0/importedOn"
                    class="Date"
                    requirementLevel = "MAY"
                />          
                <propery
                    name="Imported By"
                    predicate="http://purl.org/pav/2.0/importedBy"
                    class="Uri"
                    requirementLevel = "MAY"
                />
            </group>
            <group name = "Derived">
                <propery
                    name="Derived From"
                    predicate="http://purl.org/pav/2.0/derivedFrom"
                    class="Uri"
                    requirementLevel = "MAY"
                />          
                <propery
                    name="Derived On"
                    predicate="http://purl.org/pav/2.0/derivedOn"
                    class="Date"
                    requirementLevel = "MAY"
                />              
                <propery
                    name="Derived By"
                    predicate="http://purl.org/pav/2.0/derivedBy"
                    class="Uri"
                    requirementLevel = "MAY"
                /> 
            </group>
        </alternatives>
        <propery
            name="Vocabularies"
            predicate="http://rdfs.org/ns/void#vocabulary"
            class="Uri"
            requirementLevel = "SHOULD"
        />          
        <propery
            name="Topics"
            predicate="http://purl.org/dc/terms/subject"
            class="URI"
            requirementLevel = "MUST"
        />          
        <propery
            name="Examples"
            predicate="http://rdfs.org/ns/void#exampleResource"
            class="URI"
            requirementLevel = "MUST"
        />          
        <propery
            name="Data Dump"
            predicate="http://rdfs.org/ns/void#dataDump"
            class="URI"
            requirementLevel = "MUST"
        />          
        <propery
            name="Sparql End Point"
            predicate="http://rdfs.org/ns/void#sparqlEndpoint"
            class="Uri"
            requirementLevel = "MAY"
        />      
        <propery
            name = "Update Frequency"
            predicate = "http://voag.linkedmodel.org/schema/voag#frequencyOfChange"
            class = "AllowedUris"
            requirementLevel = "SHOULD"
        >
            <AllowedValue>http://purl.org/cld/freq/triennial</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/biennial</AllowedValue>,
            <AllowedValue>http://purl.org/cld/freq/annual</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/semiannual</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/threeTimesAYear</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/quarterly</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/bimonthly</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/monthly</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/semimonthly</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/biweekly</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/threeTimesAMonth</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/weekly</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/semiweekly</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/threeTimesAWeek</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/daily</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/continuous</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/irregular</AllowedValue>
        </propery>
  
    </resource>
    <resource name="DatasetDescription" type="http://rdfs.org/ns/void#Linkset">
         <propery
            name="Title"
            predicate="http://purl.org/dc/terms/title"
            class="String"
            requirementLevel = "MUST"
        />          
        <propery
            name="Description"
            predicate="http://purl.org/dc/terms/description"
            class="String"
            requirementLevel = "MUST"
        />          
        <propery
            name="License"
            predicate="http://purl.org/dc/terms/license"
            class="Uri"
            requirementLevel = "MUST"
        />          
       <linkedResource 
            name = "subject"
            predicate = "http://rdfs.org/ns/void#subjectsTarget"
            type="http://rdfs.org/ns/void#Dataset"
            requirementLevel = "MUST"
        />      
       <linkedResource 
            name = "object"
            predicate = "http://rdfs.org/ns/void#objectsTarget"
            type="http://rdfs.org/ns/void#Dataset"
            requirementLevel = "MUST"
        />      
        <propery
            name="Link Relationship"
            predicate="http://rdfs.org/ns/void#linkPredicate"
            class="Uri"
            requirementLevel = "MUST"
        />          
        <propery
            name="Link Justification"
            predicate="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#expresses"
            class="Uri"
            requirementLevel = "MUST"
        />          
        <!-- subset -->
        <alternatives name = "Link Source" requirementLevel = "MUST">
            <group name = "authored" >
                <propery
                    name="authored By"
                    predicate="http://purl.org/pav/2.0/authoredBy"
                    class="Uri"
                    requirementLevel = "MAY"
                />          
                <propery
                    name="authored On"
                    predicate="http://purl.org/pav/2.0/authoredOn"
                    class="Date"
                    requirementLevel = "MAY"
                />          
            </group>
            <propery
                name="Created with"
                predicate="http://purl.org/pav/2.0/createdwith"
                class="Uri"
                requirementLevel = "MAY"
            />          
        <propery
            name="Created By"
            predicate="http://purl.org/pav/2.0/createdBy"
            class="Uri"
            requirementLevel = "MUST"
            />          
        <propery
            name="Created On"
            predicate="http://purl.org/pav/2.0/createdOn"
            class="Date"
            requirementLevel = "MUST"
            />          
        </alternatives>
        <propery
            name="Linkset Statistics"
            predicate="http://rdfs.org/ns/void#triples"
            class="Integer"
            requirementLevel = "SHOULD"
        />          
    </resource>
</metadata>
