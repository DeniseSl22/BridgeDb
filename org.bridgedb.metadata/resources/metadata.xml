<?xml version="1.0" encoding="windows-1252"?>

<metadata>
    <documentationRoot>http://www.cs.man.ac.uk/~graya/ops/2012/ED-datadesc/</documentationRoot>
    <resource name="Dataset void" type="http://rdfs.org/ns/void#Dataset">
        <property
            name="Title"
            predicate="http://purl.org/dc/terms/title"
            class="String"
            requirementLevel = "MUST"
            documentation = "#dataset-metadata"
            />          
        <property
            name="Description"
            predicate="http://purl.org/dc/terms/description"
            class="String"
            requirementLevel = "MUST"
            documentation = "#dataset-metadata"
            />          
        <property
            name="Homepage"
            predicate="http://xmlns.com/foaf/0.1/homepage"
            class="Uri"
            requirementLevel = "MUST"
            documentation = "#dataset-metadata"
            />          
        <property
            name="Page"
            predicate="http://xmlns.com/foaf/0.1/page"
            class="Uri"
            requirementLevel = "MAY"
            documentation = "#dataset-metadata"
            />          
        <property
            name="License"
            predicate="http://purl.org/dc/terms/license"
            class="Uri"
            requirementLevel = "MUST"
            documentation = "#dataset-metadata"
            />          
        <property
            name="Namespace"
            predicate="http://rdfs.org/ns/void#uriSpace"
            class="String"
            requirementLevel = "MUST"
            documentation = "#dataset-metadata"
            />          
        <property
            name="Version"
            predicate="http://purl.org/pav/version"
            class="String"
            requirementLevel = "MAY"
            documentation = "#dataset-prov-version"
            />          
        <property
            name="Previous Version"
            predicate="http://purl.org/pav/previousVersion"
            class="String"
            requirementLevel = "MAY"
            documentation = "#dataset-prov-version"
            />          
        <alternatives name = "Provenance" requirementLevel = "MUST" documentation = "#dataset-prov-version">
            <group name = "Original Location" documentation = "#dataset-prov-version">
                <property
                    name="Publisher"
                    predicate="http://purl.org/dc/terms/publisher"
                    class="Uri"
                    requirementLevel = "MAY"
                    documentation = "#dataset-prov-version"
                    />          
                <property
                    name="Created"
                    predicate="http://purl.org/dc/terms/created"
                    class="http://www.w3.org/2001/XMLSchema#dateTime"
                    requirementLevel = "MAY"
                    documentation = "#dataset-prov-version"
                    />          
                <property
                    name="Modified"
                    predicate="http://purl.org/dc/terms/modified"
                    class="http://www.w3.org/2001/XMLSchema#dateTime"
                    requirementLevel = "MAY"
                    documentation = "#dataset-prov-version"
                    />
            </group>
            <group name = "Retrieved" documentation = "#dataset-prov-version">
                <property
                    name="Retrieved From"
                    predicate="http://purl.org/pav/retrievedFrom"
                    class="Uri"
                    requirementLevel = "MAY"
                    documentation = "#dataset-prov-version"
                    />          
                <property
                    name="Retrieved On"
                    predicate="http://purl.org/pav/retrievedOn"
                    class="http://www.w3.org/2001/XMLSchema#dateTime"
                    requirementLevel = "MAY"
                    documentation = "#dataset-prov-version"
                    />          
                <property
                    name="Retrieved By"
                    predicate="http://purl.org/pav/retrievedBy"
                    class="Uri"
                    requirementLevel = "MAY"
                    documentation = "#dataset-prov-version"
                    />     
            </group>
            <group name = "imported" documentation = "#dataset-prov-version">
                <property
                    name="Imported From"
                    predicate="http://purl.org/pav/importedFrom"
                    class="Uri"
                    requirementLevel = "MAY"
                    documentation = "#dataset-prov-version"
                    />          
                <property
                    name="Imported On"
                    predicate="http://purl.org/pav/importedOn"
                    class="http://www.w3.org/2001/XMLSchema#dateTime"
                    requirementLevel = "MAY"
                    documentation = "#dataset-prov-version"
                    />          
                <property
                    name="Imported By"
                    predicate="http://purl.org/pav/importedBy"
                    class="Uri"
                    requirementLevel = "MAY"
                    documentation = "#dataset-prov-version"
                    />
            </group>
            <group name = "Derived" documentation = "#dataset-prov-version">
                <property
                    name="Derived From"
                    predicate="http://purl.org/pav/derivedFrom"
                    class="Uri"
                    requirementLevel = "MAY"
                    documentation = "#dataset-prov-version"
                    />          
                <property
                    name="Created On"
                    predicate="http://purl.org/pav/createdOn"
                    class="http://www.w3.org/2001/XMLSchema#dateTime"
                    requirementLevel = "MAY"
                    documentation = "#dataset-prov-version"
                    />              
                <property
                    name="Created By"
                    predicate="http://purl.org/pav/createdBy"
                    class="Uri"
                    requirementLevel = "MAY"
                    documentation = "#dataset-prov-version"
                    /> 
            </group>
        </alternatives>
        <property
            name="Created With"
            predicate="http://purl.org/pav/createdWith"
            class="Uri"
            requirementLevel = "SHOULD"
            documentation = "#dataset-prov-version"
            /> 
        <property
            name="Vocabularies"
            predicate="http://rdfs.org/ns/void#vocabulary"
            class="Uri"
            requirementLevel = "SHOULD"
            documentation = "#dataset-topic-resources"
            />          
        <property
            name="Topics"
            predicate="http://purl.org/dc/terms/subject"
            class="String"
            requirementLevel = "MUST"
            documentation = "#dataset-topic-resources"
            />          
        <property
            name="Examples"
            predicate="http://rdfs.org/ns/void#exampleResource"
            class="URI"
            requirementLevel = "MUST"
            documentation = "#dataset-topic-resources"
            />          
        <property
            name="Data Dump"
            predicate="http://rdfs.org/ns/void#dataDump"
            class="URI"
            requirementLevel = "MUST"
            documentation = "#dataset-access"
            />          
        <property
            name="Sparql End Point"
            predicate="http://rdfs.org/ns/void#sparqlEndpoint"
            class="Uri"
            requirementLevel = "MAY"
            documentation = "#dataset-access"
            />      
        <property
            name = "Update Frequency"
            predicate = "http://voag.linkedmodel.org/schema/voag#frequencyOfChange"
            class = "AllowedUris"
            requirementLevel = "SHOULD"
            documentation = "#dataset-update-frequency"
            >
            <AllowedValue>http://purl.org/cld/freq/triennial</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/biennial</AllowedValue>,
            <AllowedValue>http://purl.org/cld/freq/annual</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/semiannual</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/threeTimesAYear</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/quarterly</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/bimonthly</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/monthly</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/semimonthly</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/biweekly</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/threeTimesAMonth</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/weekly</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/semiweekly</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/threeTimesAWeek</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/daily</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/continuous</AllowedValue>
            <AllowedValue>http://purl.org/cld/freq/irregular</AllowedValue>
            <!--http://dublincore.org/groups/collections/frequency/2007-03-09/#freqirregular also refers to http://purl.org/cld/freq/completelyIrregular -->
            <AllowedValue>http://purl.org/cld/freq/completelyIrregular </AllowedValue>
        </property>
    </resource>
    
    <resource name="Linkset Void" type="http://rdfs.org/ns/void#Linkset">
         <property
            name="Title"
            predicate="http://purl.org/dc/terms/title"
            class="String"
            requirementLevel = "MUST"
            documentation = "#linkset-metadata"
            />          
        <property
            name="Description"
            predicate="http://purl.org/dc/terms/description"
            class="String"
            requirementLevel = "MUST"
            documentation = "#linkset-metadata"
            />          
        <property
            name="License"
            predicate="http://purl.org/dc/terms/license"
            class="Uri"
            requirementLevel = "MUST"
            documentation = "#linkset-metadata"
            />          
       <linkedResource 
            name = "subject"
            predicate = "http://rdfs.org/ns/void#subjectsTarget"
            type="http://rdfs.org/ns/void#Dataset"
            requirementLevel = "MUST"
            documentation = "#link-info"
            />      
       <linkedResource 
            name = "object"
            predicate = "http://rdfs.org/ns/void#objectsTarget"
            type="http://rdfs.org/ns/void#Dataset"
            requirementLevel = "MUST"
            documentation = "#link-info"
            />      
        <!-- todo   http://rdfs.org/ns/void#target -->  
        <property
            name="Link Relationship"
            predicate="http://rdfs.org/ns/void#linkPredicate"
            class="Uri"
            requirementLevel = "MUST"
            documentation = "#link-info"
            />          
        <property
            name="Link Justification"
            predicate="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#expresses"
            class="Uri"
            requirementLevel = "MUST"
            documentation = "#link-info"
            />          
        <!-- subset handled in code as very different -->
        <alternatives name = "Link Source" requirementLevel = "MUST" documentation = "#linkset-provenance">
            <group name = "authored" documentation = "#linkset-provenance">
                <property
                    name="authored By"
                    predicate="http://purl.org/pav/authoredBy"
                    class="Uri"
                    requirementLevel = "MAY"
                    documentation = "#linkset-provenance"
                    />          
                <property
                    name="authored On"
                    predicate="http://purl.org/pav/authoredOn"
                    class="http://www.w3.org/2001/XMLSchema#dateTime"
                    requirementLevel = "MAY"
                    documentation = "#linkset-provenance"
                    />          
            </group>
            <property
                name="Created with"
                predicate="http://purl.org/pav/createdwith"
                class="Uri"
                requirementLevel = "MAY"
                documentation = "#linkset-provenance"
                />          
        <property
            name="Created By"
            predicate="http://purl.org/pav/createdBy"
            class="Uri"
            requirementLevel = "MUST"
            documentation = "#linkset-provenance"
            />          
        <property
            name="Created On"
            predicate="http://purl.org/pav/createdOn"
            class="http://www.w3.org/2001/XMLSchema#dateTime"
            requirementLevel = "MUST"
            documentation = "#linkset-provenance"
            />          
        </alternatives>
        <property
            name="Version"
            predicate="http://purl.org/pav/version"
            class="String"
            requirementLevel = "MAY"
            documentation = "#dataset-prov-version"
            />          
        <property
            name="Previous Version"
            predicate="http://purl.org/pav/previousVersion"
            class="String"
            requirementLevel = "MAY"
            documentation = "#dataset-prov-version"
            />          
        <property
            name="Linkset Statistics"
            predicate="http://rdfs.org/ns/void#triples"
            class="Integer"
            requirementLevel = "SHOULD"
            documentation = "#linkset-stats"
            />          
    </resource>
    <!-- Dataset Access

    Must declare, for each dataset that is not a parent for the linkset, when and by whom it was accessed using:
        pav:sourceAccessedBy
        pav:sourceAccessedOn
    -->
    
    <resource name="Decription void" type="http://rdfs.org/ns/void#DatasetDescription" requirementLevel = "MUST">
        <property
            name="Created By"
            predicate="http://purl.org/pav/createdBy"
            class="Uri"
            requirementLevel = "MUST"
            documentation = "#exchanging-descriptions"
            />          
        <property
            name="Created On"
            predicate="http://purl.org/pav/createdOn"
            class="http://www.w3.org/2001/XMLSchema#dateTime"
            requirementLevel = "MUST"
            documentation = "#exchanging-descriptions"
            />          
        <property
            name="Last Updated"
            predicate="http://purl.org/pav/lastUpdateOn"
            class="http://www.w3.org/2001/XMLSchema#dateTime"
            requirementLevel = "MUST"
            documentation = "#exchanging-descriptions"
            />          
        <property
            name="Primary Topic"
            predicate="http://xmlns.com/foaf/0.1/primaryTopic"
            class="URI"
            requirementLevel = "SHOULD"
            documentation = "#exchanging-descriptions"
            />          
    </resource>
    
    <resource name="Decription void" type="http://rdfs.org/ns/void#DatasetDescription" requirementLevel = "MUST">
        <property
            name="Created By"
            predicate="http://purl.org/pav/createdBy"
            class="Uri"
            requirementLevel = "MUST"
            documentation = "#exchanging-descriptions"
            />          
        <property
            name="Created On"
            predicate="http://purl.org/pav/createdOn"
            class="http://www.w3.org/2001/XMLSchema#dateTime"
            requirementLevel = "MUST"
            documentation = "#exchanging-descriptions"
            />          
        <property
            name="Last Updated"
            predicate="http://purl.org/pav/lastUpdateOn"
            class="http://www.w3.org/2001/XMLSchema#dateTime"
            requirementLevel = "MUST"
            documentation = "#exchanging-descriptions"
            />          
        <property
            name="Primary Topic"
            predicate="http://xmlns.com/foaf/0.1/primaryTopic"
            class="URI"
            requirementLevel = "SHOULD"
            documentation = "#exchanging-descriptions"
            />          
    </resource>

</metadata>
